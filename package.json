{
  "name": "@rexxars/react-split-pane",
  "description": "React split-pane component",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "source": "src/index.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "version": "0.1.93",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomkp/react-split-pane"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomkp/react-split-pane"
  },
  "homepage": "https://github.com/tomkp/react-split-pane",
  "author": "tomkp <tom@tomkp.com>",
  "keywords": [
    "react",
    "react-component",
    "split-pane",
    "react-split-pane",
    "es6"
  ],
  "scripts": {
    "start": "parcel website/index.html",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "clean": "rimraf dist",
    "dev": "vite dev",
    "test": "BABEL_ENV=test mochify -R spec --transform babelify",
    "test:watch": "BABEL_ENV=test mochify -R spec --watch --transform babelify",
    "test:coverage": "BABEL_ENV=test mochify --plugin [ mochify-istanbul --exclude '**/test/**' --report lcovonly ] --transform babelify && cat lcov.info | coveralls && rm lcov.info",
    "prettier": "prettier --write '{src,test}/**/*.js'",
    "lint": "eslint src test",
    "release": "standard-version",
    "deploy": "gh-pages -d build",
    "prepublishOnly": "npm run test && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "react-lifecycles-compat": "^3.0.4"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^5.0.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-modules-commonjs": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@types/react": "^16.9.11",
    "babel-loader": "^8.0.0-beta.6",
    "babelify": "^10.0.0",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "coveralls": "^3.0.7",
    "cross-env": "^7.0.0",
    "eslint": "^9.16.0",
    "gh-pages": "^2.1.1",
    "husky": "^4.2.3",
    "mochify": "^6.6.0",
    "mochify-istanbul": "^2.4.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^3.4.2",
    "pretty-quick": "^4.0.0",
    "react": "^19",
    "react-dom": "^19",
    "react-router-dom": "^5.1.2",
    "rimraf": "^3.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "standard-version": "^7.0.0",
    "vite": "^6.0.3"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  }
}
